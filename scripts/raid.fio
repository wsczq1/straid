[global]
ioengine=sync

filename=/dev/md5
bs=320K
numjobs=32

runtime=10
iodepth=1
time_based=1
randrepeat=0
group_reporting
zero_buffers
cpus_allowed_policy=split
direct=1
size=1G

[read]
stonewall
rw=read

[randread]
stonewall
rw=randread

[write]
stonewall
rw=write

[randwrite]
stonewall
rw=randwrite
